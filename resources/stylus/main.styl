@import '/includes/mixins'
@import '/includes/*'
@import guide

.clear
  clearfix()

body
  background-color gc(white1)
  gf(copy1)
  padding 50px 10px 0 10px
  margin 0 10px 0 10px
  //text-align justify


.burger
  display none

.container
  width 1168px
  margin auto
  min-height 200px
  position relative

.page
  min-height 200px
  margin 0 0 0 90px

.title
  gf(h2)
  text-transform uppercase
  color gc(blue1)
  margin 0 0 60px 0

.subtitle
  gf(h3)

a
  text-decoration none
  position relative

@media all and (min-width: 1px) and (max-width: 1000px)
  .burger
    display block
  body
    padding 0
    margin 0
  .container
    width auto
    margin 20px
  .page
    margin 0
  .title
    gf(h3)
    margin 0 0 20px 0


@import '/partial/*'
@import '/page/*'

